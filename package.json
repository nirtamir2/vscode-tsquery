{
  "name": "tsquery",
  "displayName": "tsquery",
  "description": "Search for TypeScript code using AST queries across the project",
  "version": "0.1.1",
  "publisher": "urishaked",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urish/vscode-tsquery.git"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onView:astView",
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onCommand:extension.astQueryFile",
    "onCommand:extension.astQueryWorkspace"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.astQueryFile",
        "title": "TSQuery AST Search in Current File"
      },
      {
        "command": "extension.astQueryWorkspace",
        "title": "TSQuery AST Search in Workspace"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "astView",
          "name": "AST View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "precommit": "lint-staged",
    "deploy": "vsce publish",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@phenomnomnominal/tsquery": "^1.0.0",
    "typewiz-core": "^1.0.2",
    "util.promisify": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier --write", "git add"],
    "*.ts": ["prettier --write", "tslint --fix", "git add"]
  }
}
