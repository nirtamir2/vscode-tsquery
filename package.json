{
    "name": "tsquery",
    "displayName": "tsquery",
    "description": "Search for TypeScript code using AST queries across the project",
    "version": "0.1.0",
    "publisher": "urishaked",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/urish/ngx-moment.git"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript",
        "onLanguage:typescriptreact",
        "onLanguage:javascriptreact",
        "onCommand:extension.astQueryFile",
        "onCommand:extension.astQueryWorkspace"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.astQueryFile",
                "title": "TSQuery AST Search in Current File"
            },
            {
                "command": "extension.astQueryWorkspace",
                "title": "TSQuery AST Search in Workspace"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "deploy": "vsce publish",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "0.1.0-beta.1",
        "typewiz-core": "^1.0.2",
        "util.promisify": "^1.0.0"
    }
}
